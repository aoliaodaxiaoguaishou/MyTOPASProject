d:Ge/Membrane/InnerY= 12.45 um
d:Ge/Membrane/InnerZ= 2.35 um
s:Ge/Membrane/Color="white"

#### CYTOPLASM ####
s:Ge/Cytoplasm/Type="TsEllipsoidalShell"
s:Ge/Cytoplasm/Material="Cytoplasm"
s:Ge/Cytoplasm/Parent="World"
d:Ge/Cytoplasm/OuterX= 28.31 um
d:Ge/Cytoplasm/OuterY= 12.45 um
d:Ge/Cytoplasm/OuterZ= 2.35 um
